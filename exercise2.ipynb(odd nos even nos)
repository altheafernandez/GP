# Initialize empty lists to store even and odd numbers
even_numbers = []
odd_numbers = []

# Iterate through integers from 1 to 50
for i in range(1, 51):
    if i % 2 == 0:
        even_numbers.append(i)
    else:
        odd_numbers.append(i)

# Print the results
print("Even numbers:", even_numbers)
print("Odd numbers:", odd_numbers)
